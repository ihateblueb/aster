{
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-sh",
		"prettier-plugin-ini",
		"@desmos/prettier-plugin-fluent"
	],
	"trailingComma": "none",
	"tabWidth": 4,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "consistent",
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"vueIndentScriptAndStyle": true
}
